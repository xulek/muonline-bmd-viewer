{
  "name": "bmd-viewer-final",
  "version": "1.1.0",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "electron": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build && electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@lunapaint/tga-codec": "^0.2.0",
    "@types/crypto-js": "^4.2.2",
    "crypto-js": "^4.2.0",
    "electron-is-dev": "^3.0.1",
    "three": "^0.181.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/three": "^0.181.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "electron": "^39.2.3",
    "electron-builder": "^26.0.12",
    "jest": "^30.2.0",
    "raw-loader": "^4.0.2",
    "style-loader": "^4.0.0",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  },
  "build": {
    "appId": "com.bmdviewer.app",
    "productName": "BMD Viewer",
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "public/vite.svg"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Graphics"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.graphics-design"
    }
  }
}
